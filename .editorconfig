# Editor configuration, see http://editorconfig.org

root = true

[*]
charset = utf-8
indent_size = 2
indent_style = space
insert_final_newline = true
max_line_length = 80
trim_trailing_whitespace = true

[*.ts]
quote_type = single

[*.yml]
quote_type = single

[*.md]
max_line_length = off
trim_trailing_whitespace = false
