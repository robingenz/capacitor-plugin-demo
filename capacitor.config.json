{
  "appId": "dev.robingenz.capacitor.plugindemo",
  "appName": "capacitor-plugin-demo",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "plugins": {
    "Badge": {
      "persist": true,
      "autoClear": true
    },
    "DatetimePicker": {
      "theme": "auto"
    },
    "LiveUpdate": {
      "appId": "259cba88-20ee-48e4-8dd2-5829d5471702",
      "autoCleanup": true
    },
    "PrivacyScreen": {
      "enable": false,
      "imageName": "Splash"
    },
    "SplashScreen": {
      "launchShowDuration": 0
    }
  },
  "cordova": {}
}
